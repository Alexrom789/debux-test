{
  "manifest_version": 2,
  "name": "debux-test",
  "description": "test devtool",
  "version": "1.0",
  "permissions": [
    "*://*/*",
    "tabs",
    "http://*/*",
    "https://*/*",
    "file:///*"
  ],
  "devtools_page": "devtools.html",
  "web_accessible_resources": [
    "hook.js"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
